<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">


<mapper namespace="Board">
<insert id="boardSave" parameterType="board">
    insert into board_table(boardWriter, boardPass, boardTitle, boardContents) values (#{boardWriter}, #{boardPass}, #{boardTitle}, #{boardContents})
</insert>
    <select id="board" resultType="board">
        select * from board_table
    </select>
    <select id="findByBoard" parameterType="String" resultType="board">
        select * from board_table where boardTitle=#{boardTitle}
    </select>
</mapper>


